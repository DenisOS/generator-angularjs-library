        "angular-cookies": "~1.0.8"<% } %><% if (includeAngularModuleSanitize) { %>,
        "angular-sanitize": "~1.0.8"<% } %>
    },
    "devDependencies": {
        "angular-mocks": "~1.0.8",
        "angular-scenario": "~1.0.8"
    }
}
